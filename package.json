{
  "name": "capapprouter",
  "version": "0.0.1",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "description": "Business Application",
  "engines": {
    "node": "^20"
  },
  "dependencies": {
    "@sap/audit-logging": "^6",
    "@sap/cds": "^8",
    "@sap/xsenv": "^5",
    "@sap/xssec": "^4",
    "debug": "^4.3",
    "express": "^4",
    "hdb": "^0.19",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.96.4",
    "sqlite": "^5.1.1"
    
  },
  "sapux": [
    "app/fiori"
  ],
  "scripts": {
    "start": "cds-serve"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "uaa": {
        "kind": "xsuaa"
      },
      "auth": {
        "[production]": {
          "strategy": "JWT"
        },
        "[development]": {
          "strategy": "mock",
          "users": {
            "joe": {
              "roles": [
                "Admin",
                "Viewer"
              ]
            },
            "julie": {
              "roles": [
                "Viewer"
              ]
            },
            "bob": {
              "roles": [
                "Viewer"
              ]
            }
          }
        }
      },
      "db": {
        "[production]": {
          "kind": "hana-cloud",
          "model": [
            "db/data-config",
            "db/data-test"
          ]
        },
        "[development]": {
          "kind": "sql",
          "model": [
            "db/data-config",
            "db/data-test"
          ]
        },
        "model": [
          "db",
          "srv"
        ]
      }
    }
  }
}